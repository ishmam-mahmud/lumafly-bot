{
  "name": "lumafly",
  "version": "1.0.2",
  "description": "Lumafly - A bot for the Asexual and Aromantic Spectrum Discord",
  "scripts": {
    "----util----": "",
    "setup:init": "yarn install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "----build/run----": "",
    "build": "esbuild --bundle --outfile=dist/main.js --platform=node --external:discord.js --external:@discordjs/rest src/core/index.ts",
    "build:watch": "yarn build --watch",
    "start": "node dist/main.js",
    "start:watch": "nodemon -x \"yarn start\"",
    "-----commands-----": "",
    "gen:command": "ts-node -T scripts/generateCommand.ts && yarn index:command",
    "index:command": "ts-node -T scripts/indexCommands.ts && yarn format",
    "-----events-----": "",
    "gen:event": "ts-node -T scripts/generateEventHandler.ts && yarn index:event",
    "index:event": "ts-node -T scripts/indexEvents.ts && yarn format",
    "----tests----": "",
    "test": "jest",
    "-----deploy-----": "",
    "deploy:commands": "yarn start --deploy"
  },
  "author": "Ishmam Mahmud",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@prisma/client": "^4.3.1",
    "discord-api-types": "^0.35.0",
    "discord.js": "^14.3.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.6",
    "esbuild": "^0.15.7",
    "handlebars": "^4.7.7",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.3",
  "prettier": {
    "singleQuote": true,
    "bracketSameLine": true
  }
}
